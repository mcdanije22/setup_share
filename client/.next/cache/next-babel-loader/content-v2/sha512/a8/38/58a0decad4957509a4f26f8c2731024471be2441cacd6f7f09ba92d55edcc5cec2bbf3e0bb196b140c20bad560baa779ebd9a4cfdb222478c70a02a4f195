deleteModalStatus, setDeleteModalStatus] = useState<boolean>(false);\n  const [tempProject, setTempProject] = useState(null);\n  const [dataLoading, isloading] = useState<boolean>(false);\n\n  const closeModal = () => {\n    setTempProject(null);\n    setIsModalVisible(false);\n  };\n  const handleOk = () => {\n    setProjectFunction(tempProject);\n    setTempProject(null);\n    setIsModalVisible(false);\n  };\n  const closeDeleteModal = () => {\n    setTempProject(null);\n    setDeleteModalStatus(false);\n  };\n\n  const deleteProject = async () => {\n    isloading(true);\n    try {\n      const resp = await axios.delete(\n        `/api/project/modify/delete/${tempProject._id}`\n      );\n      if (resp.data.updatedUser) {\n        if (\n          activeProject &&\n          tempProject &&\n          tempProject._id === activeProject._id\n        ) {\n          setProjectFunction(null);\n        }\n        setUserFunction(resp.data.updatedUser);\n        message.success(\"Project Deleted\");\n      } else if (resp.data.error) {\n        message.error(resp.data.error);\n      }\n      console.log(resp);\n    } catch (error) {\n      console.log(error);\n    }\n    isloading(false);\n    closeDeleteModal();\n  };\n\n  return (\n    <div id=\"ProjectList\">\n      <Row gutter={[48, 28]} justify=\"start\">\n        <Modal\n          title=\"Open Project Modal\"\n          visible={isModalVisible}\n          onCancel={closeModal}\n          footer={[\n            <Link href=\"/overview\" key=\"submit\">\n              <Button\n                type=\"primary\"\n                disabled={\n                  activeProject &&\n                  tempProject &&\n                  activeProject._id === tempProject._id\n                }\n                onClick={handleOk}\n              >\n                Confirm\n              </Button>\n            </Link>,\n            <Button key=\"back\" onClick={closeModal}>\n              Cancel\n            </Button>,\n          ]}\n        >\n          {activeProject &&\n          tempProject &&\n          activeProject._id === tempProject._id ? (\n            <Text>\n              <Text strong>{activeProject.name}</Text> is already open!\n            </Text>\n          ) : activeProject && tempProject ? (\n            <Text>\n              Close <Text strong>{activeProject.name}</Text> project and switch\n              to\n              <Text strong> {tempProject.name}</Text> project?\n            </Text>\n          ) : tempProject ? (\n            <Text>\n              Open <Text strong>{tempProject.name}</Text> project?\n            </Text>\n          ) : (\n            \"\"\n          )}\n        </Modal>\n        <Modal\n          title=\"Delete\"\n          visible={deleteModalStatus}\n          onCancel={closeDeleteModal}\n          footer={[\n            <Button\n              key={1}\n              danger\n              type=\"primary\"\n              loading={dataLoading}\n              onClick={deleteProject}\n            >\n              Confirm\n            </Button>,\n            <Button key={2} onClick={closeDeleteModal}>\n              Cancel\n            </Button>,\n          ]}\n        >\n          <Text>\n            Delete <Text strong>{tempProject ? tempProject.name : \"\"}</Text>{\" \"}\n            Project?\n          </Text>\n        </Modal>\n        {projects\n          .sort((a, b) => (a.dateCreated < b.dateCreated ? 1 : -1))\n          .map((project, i) => {\n            return (\n              <div key={i}>\n                <Col xs={{ span: 24 }} md={{ span: 12 }} lg={{ span: 6 }}>\n                  <Card\n                    hoverable\n                    style={{\n                      backgroundColor: `${project.thumbnailColor}`,\n                    }}\n                  >\n                    <Meta\n                      title={\n                        <PageHeader\n                          title={[\n                            <div\n                              key={1}\n                              onClick={() => {\n                                setTempProject(project);\n                                setIsModalVisible(true);\n                              }}\n                            >\n                              {project.name}\n                            </div>,\n                          ]}\n                          extra={[\n                            <DeleteOutlined\n                              key={1}\n                              onClick={() => {\n                                setTempProject(project);\n                                setDeleteModalStatus(true);\n                              }}\n                            />,\n                          ]}\n                        ></PageHeader>\n                      }\n                      description={<Tag color=\"blue\">{project.tag}</Tag>}\n                    />\n                  </Card>\n                </Col>\n              </div>\n            );\n          })}\n      </Row>\n    </div>\n  );\n};\nexport default ProjectsList;\n"]},"metadata":{},"sourceType":"module"}